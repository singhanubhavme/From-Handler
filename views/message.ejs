<%- include("partials/header.ejs") -%>
<style>
    .alert {
        margin: auto !important;
        margin-top: 2em !important;
        margin-bottom: 40vh !important;
    }
</style>
<% if(msgcode === "1") { %>
    <div class="alert alert-danger w-75 d-flex flex-column" role="alert">
        <h3 class="alert-heading">Failed!</h4>
        <hr>
        <p class="mb-0">Something went wrong in /url.</p>
        <br>
    </div>
<% } else if(msgcode === "2") { %>
    <div class="alert alert-danger w-75 d-flex flex-column" role="alert">
        <h3 class="alert-heading">Failed!</h4>
        <hr>
        <p class="mb-0">Incorrect Username/Password.</p>
        <br>
    </div>
<% } else if(msgcode === "3") { %>
    <div class="alert alert-danger w-75 d-flex flex-column" role="alert">
        <h3 class="alert-heading">Failed!</h4>
        <hr>
        <p class="mb-0">Username already Registered.</p>
        <br>
    </div>
<% } else if(msgcode === "4") { %>
    <div class="alert alert-success w-75 d-flex flex-column" role="alert">
        <h3 class="alert-heading">Success!</h4>
        <hr>
        <p class="mb-0">Form Deleted.</p>
        <br>
    </div>
<% } else if(msgcode === "5") { %>
    <div class="alert alert-danger w-75 d-flex flex-column" role="alert">
        <h3 class="alert-heading">Failed!</h4>
        <hr>
        <p class="mb-0">Cannot Delete this form.</p>
        <br>
    </div>
<% } else if(msgcode === "6") { %>
    <div class="alert alert-danger w-75 d-flex flex-column" role="alert">
        <h3 class="alert-heading">Failed!</h4>
        <hr>
        <p class="mb-0">Error 404 Nothing Here.</p>
        <br>
    </div>
<% } else if(msgcode === "7") { %>
    <div class="alert alert-danger w-75 d-flex flex-column" role="alert">
        <h3 class="alert-heading">Failed!</h4>
        <hr>
        <p class="mb-0">You do not have admin permissions to access this page.</p>
        <br>
    </div>
<% } %>

<%- include("partials/footer.ejs") -%>